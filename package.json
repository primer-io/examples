{
  "name": "primer-playground",
  "version": "0.0.0-semantic-release",
  "packageManager": "yarn@4.5.0",
  "workspaces": [
    "packages/*",
    "docs",
    "examples/*"
  ],
  "nohoist": [
    "**/react",
    "**/react-dom",
    "**/typescript",
    "**/@types/react",
    "**/@types/react-dom"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "2.5.3"
  },
  "dependencies": {
    "docusaurus-lunr-search": "3.6.0"
  }
}
